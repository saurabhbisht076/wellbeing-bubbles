{
    "compilerOptions": {
      "target": "ES2021",
      "module": "commonjs",
      "lib": ["ES2021", "DOM"],
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "react-jsx"
    },
    "exclude": ["node_modules", "dist"]
  }